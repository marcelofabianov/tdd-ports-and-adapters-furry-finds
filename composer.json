{
  "name": "marcelofabianov/furry-finds",
  "type": "project",
  "license": "MIT",
  "description": "Furry Finds",
  "keywords": [
    "fur",
    "friend",
    "market"
  ],
  "require-dev": {
    "pestphp/pest": "^2.13",
    "mockery/mockery": "^1.6",
    "pestphp/pest-plugin-faker": "^2.0",
    "pestphp/pest-plugin-type-coverage": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "FurryFinds\\App\\": "src/App/",
      "FurryFinds\\Domain\\": "src/Domain/",
      "FurryFinds\\Services\\": "src/Services/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {},
  "minimum-stability": "stable",
  "prefer-stable": true,
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  }
}
